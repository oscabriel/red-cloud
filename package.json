{
  "type": "module",
  "name": "rwsdk-guestbook",
  "version": "1.0.0",
  "description": "Guestbook app using rwsdk with alchemy",
  "main": "index.js",
  "keywords": [],
  "author": "oscar gabriel",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "clean": "rm -rf .wrangler && rm -rf node_modules && rm -rf bun.lock",
    "typecheck": "tsc --noEmit",
    "preview": "bun build && bun vite preview",
    "infra:up": "bun alchemy deploy",
    "infra:down": "bun alchemy destroy",
    "db:touch": "wrangler d1 execute DB --local --command='SELECT 1'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.4",
    "rwsdk": "^0.1.27"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250620.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "alchemy": "0.55.3",
    "drizzle-kit": "^0.31.4",
    "miniflare": "^4.20250617.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "wrangler": "^4.16.0"
  },
  "packageManager": "bun@1.2.18"
}
