{
	"type": "module",
	"name": "rwsdk-guestbook",
	"version": "1.0.0",
	"description": "Guestbook app using rwsdk with alchemy",
	"main": "index.js",
	"keywords": [],
	"author": "oscar gabriel",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "bun clean:vite && bun --env-file=.env.dev vite dev",
		"dev:init": "bun db:touch && bun db:generate && bun db:migrate",
		"build": "bun clean:vite && vite build",
		"clean:vite": "rm -rf ./node_modules/.vite",
		"clean:wrangler": "rm -rf .wrangler",
		"clean:all": "bun clean:vite && bun clean:wrangler && rm -rf node_modules && rm -rf bun.lock",
		"typecheck": "tsc --noEmit",
		"check": "biome check --write",
		"check:unsafe": "biome check --unsafe",
		"preview": "bun run build && bun vite preview",
		"infra:up": "bun alchemy deploy --env-file .env.prod",
		"infra:down": "bun alchemy destroy --env-file .env.prod",
		"db:touch": "wrangler d1 execute DB --local --command='SELECT 1'",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply DB --local",
		"db:migrate:prod": "wrangler d1 migrations apply DB --remote",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts",
		"db:studio:prod": "bun --env-file=.env.prod drizzle-kit studio"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.11",
		"@tanstack/react-form": "^1.15.1",
		"better-auth": "^1.3.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.4",
		"input-otp": "^1.4.2",
		"lucide-react": "^0.536.0",
		"radix-ui": "^1.4.2",
		"rwsdk": "0.1.32",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"ua-parser-js": "^2.0.4",
		"zod": "^4.0.14"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.3",
		"@cloudflare/workers-types": "^4.20250807.0",
		"@types/node": "^24.2.0",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"alchemy": "0.57.2",
		"drizzle-kit": "^0.31.4",
		"miniflare": "^4.20250617.3",
		"tw-animate-css": "^1.3.6",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"wrangler": "^4.16.0"
	},
	"packageManager": "bun@1.2.19",
	"trustedDependencies": [
		"@swc/core",
		"@tailwindcss/oxide",
		"esbuild",
		"sharp",
		"workerd"
	]
}
